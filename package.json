{
  "name": "web-vue",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint   --fix --ext  .vue,.js,.ts,.jsx,.tsx,.md,.json ",
    "lint-staged":"lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "element-plus": "^2.2.9",
    "pinia": "^2.0.16",
    "vue": "^3.2.25",
    "vue-router": "^4.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-vue": "^2.3.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.20.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "vite": "^2.9.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{vue,ts,js,jsx,json,md}": [
      "npm run lint"
    ]
  }
}
